{
  "expo": {
    "name": "hanaplingkod",
    "slug": "hanaplingkod",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/logo/logo_icon.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/logo/logo_full.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/logo/logo_icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "permissions": [
        "android.permission.RECORD_AUDIO"
      ],
      "package": "com.aceeyou.hanaplingkod"
    },
    "web": {
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to let you share them with your friends."
        }
      ],
      [
        "expo-notifications",
        {
          "icon": "./assets/logo/logo_icon.png",
          "color": "#2080ff",
          "sounds": []
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "8be07aa2-6f64-43dd-b0ac-d4a17c86a5a0"
      }
    }
  }
}
